  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*",
    "eslintrc.*"
  ],
  "engines": {
    "node": ">=16.9"
  },
  "packageManager": "yarn@3.2.2",
  "private": true,
  "dependenciesMeta": {
    "@prisma/client@4.8.1": {
      "unplugged": true
    },
    "prisma@4.8.1": {
      "unplugged": true
