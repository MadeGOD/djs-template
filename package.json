    "lint:fix": "prettier --write '**/*.ts' && eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@prisma/client": "4.8.1",
    "chalk": "5.2.0",
    "discord.js": "^14.7.1",
    "dokdo": "^0.6.2",
    "dotenv": "^16.0.3",
    "node-fetch": "3.3.0",
    "strip-ansi": "^7.0.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@yarnpkg/pnpify": "^4.0.0-rc.35",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-require-extensions": "^0.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prisma": "4.8.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
